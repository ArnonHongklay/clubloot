<% content_for :body_id, 'edit-template' %>

<article class="content item-editor-page">
  <div class="title-block">
    <h3 class="title">
      Editing Template <span class="sparkline bar" data-type="bar"></span>
    </h3>
  </div>
  <%= link_to 'Show', @template, class: 'btn btn-primary btn-sm rounded-s' %>
  <%= link_to 'Back', templates_path, class: 'btn btn-primary btn-sm rounded-s' %>
  <%= render 'form', template: @template %>
</article>
